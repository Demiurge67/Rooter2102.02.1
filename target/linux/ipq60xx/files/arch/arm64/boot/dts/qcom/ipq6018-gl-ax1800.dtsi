&soc {
    dp1: dp@1 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		reg = <0x0 0x3a001000 0x0 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [000000000000];
		qcom,id = <1>;
		phy-handle = <&qca8075_0>;
		phy-mode = "sgmii";
	};

    dp2: dp@2 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		reg = <0x0 0x3a001200 0x0 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [000000000000];
		qcom,id = <2>;
		phy-handle = <&qca8075_1>;
		phy-mode = "sgmii";
	};

    dp3: dp@3 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		reg = <0x0 0x3a001400 0x0 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [000000000000];
		qcom,id = <3>;
		phy-handle = <&qca8075_2>;
		phy-mode = "sgmii";
	};

};

&qpic_bam {
	status = "okay";
};

&qpic_nand {
	status = "okay";

	nand@0 {
		reg = <0>;

		nand-ecc-strength = <4>;
		nand-ecc-step-size = <512>;
		nand-bus-width = <8>;

        partitions {
			compatible = "qcom,smem-part";
		};
	};
};

&cpu_opp_table {
	/delete-node/ opp-1440000000;
	/delete-node/ opp-1608000000;
	/delete-node/ opp-1800000000;	
	};

&wifi {
	status = "okay";

	qcom,ath11k-calibration-variant = "GL-iNet-AX1800";
};