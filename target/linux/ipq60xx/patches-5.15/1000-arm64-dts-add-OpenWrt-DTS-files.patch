+dtb-$(CONFIG_ARCH_QCOM)	+= ipq6018-gl-axt1800.dtb
+dtb-$(CONFIG_ARCH_QCOM)	+= ipq6018-eap610-outdoor.dtb
+dtb-$(CONFIG_ARCH_QCOM)	+= ipq6018-sxk30.dtb
+dtb-$(CONFIG_ARCH_QCOM)	+= ipq6018-wax610.dtb
 dtb-$(CONFIG_ARCH_QCOM)	+= ipq8074-hk01.dtb
